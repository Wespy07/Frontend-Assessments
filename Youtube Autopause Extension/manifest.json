{
    "name": "Youtube Auto Pause",
    "description": "Pause youtube videos as soon as you switch tabs",
    "version": "1.3.091",
    "manifest_version": 3,
    "permissions": ["scripting"],
    "host_permissions": ["https://www.youtube.com/*"],
    "content_scripts": [
        {
          "matches": ["https://www.youtube.com/*"],
          "js": ["script.js"]
        }
      ],
      "icons":{
        "16" : "logo.png",
        "48" : "logo.png",
        "128" : "logo.png"
      }
}